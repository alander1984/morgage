      <th>Act19</th>
      <th>Act20</th>
      <th>Actothertext</th>
      <th>Cnt1</th>
      <th>Cnt2</th>
      <th>Cnt3</th>
      <th>Cnt4</th>
      <th>Cnt5</th>
      <th>Cnt6</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @works.each do |work| %>
      <tr>
        <td><%= work.name %></td>
        <td><%= work.index %></td>
        <td><%= work.region %></td>
        <td><%= work.area %></td>
        <td><%= work.city %></td>
        <td><%= work.street %></td>
        <td><%= work.house %></td>
        <td><%= work.corp %></td>
        <td><%= work.str %></td>
        <td><%= work.office %></td>
        <td><%= work.phone %></td>
        <td><%= work.site %></td>
        <td><%= work.act1 %></td>
        <td><%= work.act2 %></td>
        <td><%= work.act3 %></td>
        <td><%= work.act4 %></td>
        <td><%= work.act5 %></td>
        <td><%= work.act6 %></td>
        <td><%= work.act7 %></td>
        <td><%= work.act8 %></td>
        <td><%= work.act9 %></td>
        <td><%= work.act10 %></td>
        <td><%= work.act11 %></td>
        <td><%= work.act12 %></td>
        <td><%= work.act13 %></td>
        <td><%= work.act14 %></td>
        <td><%= work.act15 %></td>
        <td><%= work.act16 %></td>
